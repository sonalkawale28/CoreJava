package patterns;

public class P3 
{
	public static void main(String[] args) {
		
		for(int i=1;i<=5;i++) //Outer loop for no. of rows in our case 5
		{
			for(int j=4;j>=i;j--)
				{System.out.print("_");}
			for(int j=1;j<=i;j++)
				{System.out.print("*");}
			System.out.println();
		}
	}
} 
		 

	
